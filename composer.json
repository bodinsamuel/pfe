{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
    "repositories": {
        "seloger-php": {
            "type": "package",
            "package": {
                "name": "bodinsamuel/seloger-php",
                "version": "1.0.0",
                "source": {
                    "url": "https://github.com/bodinsamuel/seloger-php",
                    "type": "git",
                    "reference": "7bc59df4c20a2672ce0f1e3eee75ee315ae780f0"
                }
            }
        }
    },
	"require": {
		"laravel/framework": "4.1.*",
        "bodinsamuel/seloger-php": "1.0.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
            "app/services"
		],
        "psr-0": {
            "Custom": "app/libraries",
            "Services": "app/services",
            "Seloger": "vendor/bodinsamuel/seloger-php/src"
        }
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
